{
  "quantum_storage_config": {
    "version": "1.0.0",
    "system": {
      "base_path": "./quantum_storage",
      "physical_limit_gb": 5,
      "enable_auto_optimization": true,
      "enable_background_processing": true,
      "log_level": "INFO"
    },
    "quantum_optimization": {
      "enable_quantum_multiplication": true,
      "max_multiplier_factor": 10.0,
      "entanglement_threshold": 0.5,
      "coherence_time_hours": 24.0,
      "superposition_iterations": 100,
      "enable_quantum_compression": true,
      "quantum_replication_factor": 3
    },
    "machine_learning": {
      "enable_ml_optimization": true,
      "learning_rate": 0.01,
      "training_interval_hours": 1,
      "prediction_horizon_days": 7,
      "min_confidence_threshold": 0.7,
      "enable_neural_networks": true,
      "enable_pattern_recognition": true
    },
    "compression": {
      "enable_compression": true,
      "enable_deduplication": true,
      "enable_sparse_files": true,
      "default_algorithm": "ML_OPTIMIZED",
      "compression_levels": {
        "lz4_fast": 1,
        "lz4_high": 12,
        "zstd_fast": 1,
        "zstd_balanced": 3,
        "zstd_max": 22,
        "brotli_fast": 1,
        "brotli_balanced": 6,
        "brotli_max": 11
      },
      "block_size_kb": 64,
      "enable_ml_algorithm_selection": true
    },
    "virtual_storage": {
      "enable_virtual_fs": true,
      "enable_intelligent_tiering": true,
      "cache_size_mb": 1024,
      "tier_promotion_threshold": 10,
      "tier_demotion_threshold": 168,
      "enable_quantum_tiering": true,
      "storage_tiers": ["HOT", "WARM", "COOL", "FROZEN", "QUANTUM_TIER"]
    },
    "analytics": {
      "enable_analytics": true,
      "enable_real_time_monitoring": true,
      "data_retention_days": 30,
      "monitoring_interval_seconds": 60,
      "enable_predictive_analytics": true,
      "enable_anomaly_detection": true,
      "generate_insights": true,
      "max_insights_per_category": 10
    },
    "cloud_integration": {
      "enable_cloud_storage": true,
      "enable_auto_sync": true,
      "max_concurrent_uploads": 3,
      "max_concurrent_downloads": 5,
      "sync_interval_minutes": 15,
      "enable_multi_cloud": true,
      "enable_quantum_cloud": true,
      "providers": {
        "azure_blob": {
          "enabled": false,
          "tier_mapping": {
            "HOT": "hot",
            "WARM": "cool",
            "COOL": "cool",
            "FROZEN": "archive"
          }
        },
        "aws_s3": {
          "enabled": false,
          "tier_mapping": {
            "HOT": "standard",
            "WARM": "standard_ia",
            "COOL": "onezone_ia",
            "FROZEN": "glacier"
          }
        },
        "google_cloud": {
          "enabled": false,
          "tier_mapping": {
            "HOT": "standard",
            "WARM": "nearline",
            "COOL": "coldline",
            "FROZEN": "archive"
          }
        },
        "quantum_cloud": {
          "enabled": true,
          "enable_quantum_encryption": true,
          "enable_quantum_deduplication": true
        }
      }
    },
    "security": {
      "enable_encryption": true,
      "enable_quantum_encryption": true,
      "encryption_algorithm": "AES-256-GCM",
      "enable_integrity_checking": true,
      "enable_secure_deduplication": true
    },
    "performance": {
      "io_thread_pool_size": 4,
      "compression_thread_pool_size": 2,
      "analytics_thread_pool_size": 1,
      "cloud_sync_thread_pool_size": 3,
      "enable_async_operations": true,
      "max_memory_usage_mb": 2048,
      "enable_performance_monitoring": true
    },
    "advanced": {
      "enable_experimental_features": true,
      "enable_quantum_algorithms": true,
      "enable_advanced_ml": true,
      "debug_mode": false,
      "verbose_logging": false,
      "enable_profiling": false
    }
  }
}